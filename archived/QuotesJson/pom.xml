	<dependency>
	    <groupId>com.fasterxml.jackson.core</groupId>
	    <artifactId>jackson-databind</artifactId>
	    <version>2.9.10.5</version>
	</dependency>
	<dependency>
	    <groupId>com.oracle.antlr-runtime</groupId>
	    <artifactId>antlr</artifactId>
	    <version>4.3.3</version>
	</dependency>
	<dependency>
	    <groupId>com.oracle.avro</groupId>
	    <artifactId>avro</artifactId>
	    <version>1.0</version>
	</dependency>
	<dependency>
	    <groupId>com.oracle.jackson</groupId>
	    <artifactId>jackson-core-asl-core</artifactId>
	    <version>1.0</version>
	</dependency>
	<dependency>
	    <groupId>com.oracle.jackson</groupId>
	    <artifactId>jackson-mapper</artifactId>
